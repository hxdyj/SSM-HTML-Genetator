UserExample e = new UserExample();
		Criteria c = e.createCriteria();
		c.andNumEqualTo(num);
		List<User> list = userMapper.selectByExample(e);
		if(list.isEmpty()){
			userMapper.updateByPrimaryKey(o);
		return Util.getResult(1, "success","");

		}else{
			return Util.getResult(0, "success","用户已存在");
		}
